<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.warpgatetechnologies.junglereservations">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.warpgatetechnologies.junglereservations.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.warpgatetechnologies.junglereservations.ReservationActivity"
            android:label="Request Reservation"/>
        <activity android:name="com.warpgatetechnologies.junglereservations.CalendarActivity"
            android:label="Current Reservations"></activity>
    </application>

</manifest>